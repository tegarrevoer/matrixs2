            os.sys.exit()
    except requests.exceptions.ConnectionError:
    	os.system('clear')
        print '\x1b[1;91m [!] Tidak Ada Koneksi Data\x1b[0;97m'
        os.sys.exit()
    except KeyboardInterrupt:
        os.sys.exit()
    except IOError:
        subprocess.Popen(['rm', '-rf', 'licensed.log'])
        romz()
        
def jalan(z):
	for e in z + '\n':
		sys.stdout.write(e)
		sys.stdout.flush()
		time.sleep(0.03)
		
